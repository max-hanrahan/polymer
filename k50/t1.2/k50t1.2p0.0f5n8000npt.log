restart 1000 k50t1.2p${p}f${f}n${n}npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f${f}n${n}npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n${n}npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n8000npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n8000npt-1.res k50t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n8000npt-1.res k50t1.2p${p}f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n8000npt-1.res k50t1.2p0.0f${f}n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n8000npt-1.res k50t1.2p0.0f5n${n}npt-2.res
restart 1000 k50t1.2p0.0f5n8000npt-1.res k50t1.2p0.0f5n8000npt-2.res

#run_style respa 2 3 bond 1 pair 2
#timestep 0.006

timestep 0.002

