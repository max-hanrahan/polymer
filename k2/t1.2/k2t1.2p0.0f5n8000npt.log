restart 1000 k2t1.2p0.0f${f}n${n}npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k2t1.2p0.0f5n${n}npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k2t1.2p0.0f5n8000npt-1.res k${k}t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k2t1.2p0.0f5n8000npt-1.res k2t${T}p${p}f${f}n${n}npt-2.res
restart 1000 k2t1.2p0.0f5n8000npt-1.res k2t1.2p${p}f${f}n${n}npt-2.res
restart 1000 k2t1.2p0.0f5n8000npt-1.res k2t1.2p0.0f${f}n${n}npt-2.res
